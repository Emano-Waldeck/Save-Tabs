{
  "manifest_version": 2,
  "version": "0.1.3",
  "name": "Save Tabs",
  "description": "A password protected tab saver extension to store tabs in the current window, or all windows",
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "safe.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "data/popup/index.html"
  },
  "homepage_url": "https://add0n.com/save-tabs.html"
}
